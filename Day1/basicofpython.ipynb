   "source": [
    "Write a Python function that takes a list of numbers and returns the square of each number."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "squared Numbers are  [4, 16, 36, 64, 49]\n"
     ]
    }
   ],
   "source": [
    "def square_numbers(numbers):\n",
    "    return[x**2 for x in numbers]\n",
    "result=square_numbers([2,4,6,8,7])\n",
    "print(\"squared Numbers are \", result)"
   ]
